<p id="notice"><%= notice %></p>

<h1>Listing Api V1 Event Visitors</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Company</th>
      <th>Position</th>
      <th>Department</th>
      <th>Mobile phone</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @event_visitors.each do |api_v1_event_visitor| %>
      <tr>
        <td><%= api_v1_event_visitor.name %></td>
        <td><%= api_v1_event_visitor.company %></td>
        <td><%= api_v1_event_visitor.position %></td>
        <td><%= api_v1_event_visitor.department %></td>
        <td><%= api_v1_event_visitor.mobile_phone %></td>
        <td><%= api_v1_event_visitor.email %></td>
        <td><%= link_to 'Show', api_v1_event_visitor %></td>
        <td><%= link_to 'Edit', edit_api_v1_event_visitor_path(api_v1_event_visitor) %></td>
        <td><%= link_to 'Destroy', api_v1_event_visitor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event visitor', new_api_v1_event_visitor_path %>
